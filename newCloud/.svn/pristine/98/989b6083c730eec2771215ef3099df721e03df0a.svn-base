									}
								}
								htmls+='</li>';
				}
				$('.user_table .ta_list').html(htmls);
				$(".pager").css("display","block");
				make();

			}else if(data.code==201){
				$(".pager").css("display","none");
				msg(data.msg,false);
			}else if(data.code==202){
				$(".pager").css("display","none");
				msg(data.msg,false);
			}
		}
	});
}
