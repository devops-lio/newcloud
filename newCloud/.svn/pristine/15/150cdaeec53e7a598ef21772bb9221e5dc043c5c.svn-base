.title{
	height: 54px;
	color: #57c6d4;
	font-size: 14px;
	line-height: 54px;
	width: 100%;
	background: #f7f7f7;
	padding-left: 2%;
}
.label{
	width: 240px;
	height: 34px;
	margin-left: 2%;
	margin-top: 30px;
}
.label > li{
	float: left;
	width: 118px;
	height: 32px;
	border: 1px solid #ccc;
	text-align: center;
	line-height: 34px;
	cursor: pointer;
	color: #ccc;
}
.label > li:hover{
	color: #999;
}
.label > li.on{
	width: 120px;
	height: 34px;
	background: #57c6d4;
	color: #fff;
	border: none;
}
.label > li.on:hover{
	background: #5bd2e1;
}
.ui-content{
	margin-left: 2%;
	width: 95%;
}
.Withdraw{
	height: 88px;
	background: #f5f5f5;
	width: 100%;
	font-size: 14px;
	padding-top: 10px;
	border-top: 3px solid #ccc;
	margin-top: 3px;
}
.Withdraw > p{
	height: 30px;
	line-height: 30px;
	margin: 5px 0;
	color: #999;
}
.Withdraw > p > button{
	width: 120px;
	height: 30px;
	background: #57c6d4;
	border: none;
	color: #fff;
	margin-left: 20px;
	font-size: 12px;
	cursor: pointer;
}
.Withdraw > p > button:hover{
	background: #5bd2e1;
}
.sumMoney{
	color: #cabc43;
	font-size: 20px;
}
.condition{
	height: 34px;
	width: 100%;
	margin-top: 10px;
	line-height: 34px;
	color: #999;
}
.condition > p{
	display: inline-block;
	margin-left: 100px;
}
.condition > p > input{
	width: 98px;
	height: 32px;
	border: 1px solid #ccc;
}
.condition > p > button{
	width: 110px;
	height: 34px;
	background: #57c6d4;
	border: none;
	margin-left: 30px;
	color: #fff;
	cursor: pointer;
}
.condition > p > button:hover{
	background: #5bd2e1;
}
.watercourse{
	width: 100%;
	height: 300px;
	margin-top: 10px;
}

.watercourse > h6{
	color: #666;
	line-height: 38px;
	background: #f7f7f7;
	width: 100%;
	height: 38px;
	margin-top: 2px;
}
.watercourse > h6 > span{
	width: 11.111%;
	float: left;
	text-align: center;
}
.selec2 > h6 > span{
	width: 20%;
}
.watercourse > ul{
	width: 100%;
	height: 260px;
}
.watercourse > ul > li{
	width: 100%;
	height: 39px;
	border-bottom: 1px solid #f7f7f7;
	position: relative;
}
.watercourse > ul > li > span{
	width: 10%;
	height: 100%;
	word-wrap: break-word;
	word-break: normal;
	margin: 0 0.555%;
	display: block;
	text-align: center;
	float: left;
	font-size: 12px;
}
.selec2 > ul > li > span{
	width: 20%;
	margin: 0;
}
.watercourse > ul > li > span > i{
	font-style: normal;
	color: #6fd7e4;
	cursor: pointer;
}
.alertVerify{
	width: 478px;
	height: 126px;
	background: #fff;
	position: absolute;
	top: 50%;
	left: 50%;
	margin-left: -239px;
	margin-top: -63px;
	display: none;
}
.alertVerify > p{
	width: 100%;
	height: 30px;
	line-height: 30px;
	color: #666;
	font-size: 12px;
	text-align: center;
}
.verifyinput{
	margin-top: 30px;
}
.verifyinput > input,
.verifyinput > label,
.verifyinput > button{
	float: left;
}
.verifyinput > label{
	margin-left: 25px;
}
.verifyinput > input{
	width: 158px;
	height: 28px;
	border: 1px solid #6fd7e4;
	color: #333;
	margin-left: 20px;
	text-align: center;
}
.verifyinput > button{
	width: 122px;
	height: 30px;
	background: #e5e5e5;
	border: none;
	color: #b3b3b3;
	display: inline-block;
	cursor: pointer;
	font-size: 12px;
}
.verifyinput > button.on{
	background: #57c6d4;
	color: #fff;
}
.verifyinput > button.on:hover{
	background: #5bd2e1;
}
.verifyBtn{
	margin-top: 10px;
}
.ui-btn{
	width: 100px;
	height: 30px;
	background: #57c6d4;
	border: none;
	color: #fff;
	cursor: pointer;
	font-size: 12px;
}
.ui-btn:hover{
	background: #5bd2e1;
}
.ui-btn:nth-child(2){
	width: 98px;
	height: 30px;
	background: none;
	border: 1px solid #57c6d4;
	color: #57c6d4;
}
.ui-btn:nth-child(2):hover{
	color: #5bd2e1;
}
.show{
	width: 480px;
	height: 466px;
	position: absolute;
	top: 50%;
	left: 50%;
	overflow: hidden;
	margin-top: -233px;
	margin-left: -240px;
	background: #fff;
}
.show > h4{
	width: 100%;
	height: 44px;
	font-size: 14px;
	color: #808080;
	line-height: 44px;
	text-align: center;
	background: #f5f5f5;
	position: relative;
}
.show > h4 > i{
	font-size: 12px;
	font-weight: normal;
	position: absolute;
	top: 16px;
	right: 16px;
	cursor: pointer;
}
.show > form{
	width: 80%;
	height: 100%;
	margin: 0 auto;
}
.show > form > p{
	width: 75%;
	height: 50px;
	line-height: 50px;
	color: #808080;
	font-size: 12px;
	position: relative;
}
.show > form > p > span{
	font-size: 14px;
	color: #bcaf3e;
}
.show > form > p > input{
	width: 210px;
	height: 30px;
	background: none;
	border: 1px solid #ccc;
	float: right;
	margin-top: 10px;
	text-align: center;
	color: #999;
	font-family: Microsoft Yahei;
	font-size: 12px;
}
.selecter{
	position: absolute;
	right: -80px;
	background: none;
	border: none;
	color: #57c6d4;
	font-style: normal;
	cursor: pointer;
}
.refer{
	width: 120px;
	height: 30px;
	background: #57c6d4;
	border: none;
	color: #fff;
	margin-top: 10px;
	margin-left: 76px;
	cursor: pointer;
}
.refer:hover{
	background: #5bd2e1;
}
.listClosedid{
	width: 90%;
	height: 350px;
	margin: 0 auto;
	margin-top: 50px;
}
.addClosedid,.ensure{
	width: 120px;
	height: 30px;
	cursor: pointer;
	color: #fff;
	background: #57c6d4;
	border: none;
}
.addClosedid:hover,
.ensure:hover{
	background: #5bd2e1;
}
.closedidList{
	width: 100%;
	height: 260px;
	overflow: auto;
	margin-top: 20px;
}
.closedidList > li{
	height: 34px;
	border-bottom: 1px solid #e5e5e5;
	line-height: 34px;
	position: relative;
	cursor: pointer;
}
.closedidList > li span{
	float: left;
	display: inline-block;

}
.text{
	width: 50%;
	height: 100%;
	text-align: center;
}
.ico{
	width: 16px;
	height: 16px;
	border: 2px solid #ccc;
	border-radius: 50%;
	position: absolute;
	top: 7px;
	text-align: center;
	line-height: 22px;
}
.ico > i{
	color: #fff;
	font-size: 12px;
}
.ico:nth-child(4){
	border: none;
	background: #de7f7f;
	width: 20px;
	height: 20px;
	right: 5px !important;
	display: none;
}
.closedidList > li.on > .ico:nth-child(1){
	border: 2px solid #6fd7e4;
	line-height: 16px;
	
}
.closedidList > li.on > .ico:nth-child(1) > i{
	color: #6fd7e4;
}
.closedidList > li:hover > .ico:nth-child(4){
	display: block;
}
.ensure{
	display: block;
	margin: 0 auto;
	margin-top: 10px;
}
.poundage{
	padding: 6px 20px;
	position: absolute;
	left: 360px;
	background: #fff;
	top: 30px;
	text-align: center;
	color: #da6b6b;
	z-index: 2;
	box-shadow: 0 0 5px #999;
	display: none;
	font-size: 12px;
}
.gatheringNum{
	padding: 6px 20px;
	color: #666;
	font-size: 12px;
	text-align: center;
	box-shadow: 0 0 5px #999;
	background: #fff;
	position: absolute;
	top: 30px;
	left: 250px;
	z-index: 2;
	display: none;
}
.cause{
	padding: 6px 20px;
	box-shadow: 0 0 5px #999;
	background: #fff;
	position: absolute;
	left: 800px;
	font-size: 12px;
	top: 26px;
	z-index: 2;
	display: none;
}