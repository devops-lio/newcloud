/* --------------开关样式 start----------------- */
.yesOrNo{
	width: 48px;
    height: 18px;
    background: #bbb;
    border-radius: 22px;
    display: inline-block;
    line-height: 18px;
    position: relative;
    color: #f2f2f2;
    cursor: pointer;
    font-size: 10px;
    float: left;
    margin: 4px auto;
    letter-spacing: 0px;
}
.yesOrNo>.yes{
	margin-left: 7px;
	float: left;
	display: none;
}
.yesOrNo>.no{
	margin-right: 7px;
	float: right;
	color: #fff;
}
.yesOrNo>.bal{
	width: 16px;
    height: 16px;
    position: absolute;
    background: #f2f2f2;
    border-radius: 50%;
    top: 1px;
    left: 1px;
    box-shadow: 0 0 5px #666;
    display: inline-block;
}
.yesOrNo.on{
	background: #2CB3C4;
}
.yesOrNo.on>.yes{
	display: inline-block;
}
.yesOrNo.on>.no{
	display: none;
}
.yesOrNo.on>.bal{
	left: 31px;
}
