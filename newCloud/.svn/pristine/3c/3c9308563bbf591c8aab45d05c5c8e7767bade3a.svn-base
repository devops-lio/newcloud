//作废套餐类型
function updateStop(){
	var name=$("#priceUl .on").text();
	var id=$("#priceUl .on").attr("siteid");
	$.ajax({
		type:"POST",
		url:ctx+"/SitePriceBilling/updateIsStop",
		data:{
			name:name,
			id:id
		},
		success:function(data){
			eval("data="+data);
			if(data.code==200){
				flag=1;
				$(".win>span").html("更改成功");
				win();
				getPrice(id);
				wouldClick();
			}else if(data.code==201){
//				$(".win>span").html("该套餐未开启");
//				win();
			}else{
				$(".win>span").html("服务不可用,请稍后尝试");
				win();
				return false;
				
			}
		}
		
	});
}
