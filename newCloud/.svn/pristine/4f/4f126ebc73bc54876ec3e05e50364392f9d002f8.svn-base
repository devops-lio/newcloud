}
.ms_list > span > i{
	color: #57c6d4;
	font-weight: bold;
}
.ms_list_info span,
.ms_list > p > span{
	display: inline-block;
	text-align: center;
	width: 16.666%;
}
.ms_list > p{
	margin-top: 23px;
}
.ms_list{
	display: none;
}
.mx_unquery{
	float: left;
	margin-left: 20px;
	line-height: 30px;
}
.mx_unquery > input{
	float: left;
	height: 30px;
	width: 148px;
	text-indent: 10px;
	border: 1px solid #57c6d4;
}
.mx_box .dev_se{
	height: 60px;
	margin-left: 0;
	margin-right: 20px;
}
.mx_box .dev_se > span{
	width: 160px;
	float: left;
}
.mx_date > span{
	float: left;
}
.mx_date input{
	margin: 0;
}
.dev_se ul{
	width: 160px !important;
	left: 0;
	top: 59px;
}
.type_se{
	width: 102px !important;
}
.type_se > span{
	float: left;
}
.type_se ul{
	left: 0;
	top: 59px;
}
.ms_list_info li{
	margin:12px auto;
}

/* 收入明细 */

