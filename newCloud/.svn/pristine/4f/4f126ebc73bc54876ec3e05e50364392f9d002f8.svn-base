/* -------------------选项卡------------------------- */
.cn_table{
	color: #333;
	font-weight: bold;
	height: 30px;
	line-height: 30px;
}
.cn_table > span{
	display: inline-block;
	padding: 0 10px;
	cursor: pointer;
	margin-right: 20px;
}
.cn_table > span:hover{
	color: #57c6d4;
}
.cn_table > span.on{
	color: #57c6d4;
	border-bottom: 4px solid #57c6d4;
}
/* -------------------选项卡------------------------- */

/* 提现信息 */
.with_info{
	width: 100%;
	height: 94px;
	background: #ebf8fa;
	margin-top: 18px;
}
.with_info > p{
	height: 47px;
	line-height: 47px;
	color: #666;
	font-size: 14px;
	font-weight: bold;
	padding-left: 20px;
}
.with_info > p > img{
	margin-right: 17px;
}
.with_info > p > button{
	margin-left: 50px;
}
.with_info > p > span{
	color: #57c6d4;
}
/* 提现信息 */

/* 选项卡和查询 */
.tx_cont{
	margin-top: 23px;
	height: 30px;
}
.tx_tab{
	height: 100%;
	line-height: 30px;
	float: left;
}
.tx_tab > span{
	width: 54px;
	height: 100%;
	display: inline-block;
	line-height: 30px;
	text-align: center;
	background: #f4f4f4;
	color: #666;
	margin-right: 2px;
	cursor: pointer;
	font-size: 12px;
}
.tx_tab > span.on{
	background: #57c6d4;
	color: #fff;
}
.date_query{
	float: right;
}

/* 选项卡和查询 */

/* 提现申请列表 */
.ls_list:before,
.ms_list:before,
.tx_list:before{
	content: '单位（元）';
	float: right;
	color: #57c6d4;
	margin-top: -20px;
}
.ms_list,
.tx_list,
.ls_list{
	font-size: 12px;
	margin-top: 40px;
}
.ms_list > p,
.tx_list > p,
.ls_list > p{
	line-height: 40px;
	font-weight: bold;
	color: #666;
	background: #f7f7f7;
}
.tx_list > p > span{
	display: inline-block;
	text-align: center;
}
.tx1{
	width: 16.15%;
}
.tx2{
	width: 20.15%;
}
.tx3,
.tx4,
.tx5,
.tx6,
.tx7,
.tx8{
	width: 8.5%;
}
.tx9{
	width: 12.7%;
}
.txk{
	width: 25.5%;
}
.tx_list_info span{
	text-align: center;
	line-height: 40px;
	display: inline-block;
}
.tx_list_info li{
	height: 40px;
	border-bottom: 1px solid #f7f7f7;
}
.tx_list_info i{
	margin: 0 2px;
}
.tx_list_info img{
	margin: 5px;
}

.ckpz{
	position: relative;
}
.ckpz > .float{
	left: 50%;
	margin-left: -110px;
	display: none;
}
/* 提现申请列表 */

/* 提现申请 */
.ask_for{
	width: 426px;
	height: 326px;
	background: #fff;
	position: absolute;
	z-index: 1000;
	left: 50%;
	top: 50%;
	margin-left: -213px;
	margin-top: -163px;
	font-size: 12px;
	display: none;
}
.ax_tit{
	height: 40px;
	color: #57c6d4;
	font-weight: bold;
	background: #f7f7f7;
	line-height: 40px;
	padding-left: 20px;
	letter-spacing: 1px;
}
.ax_tit > img{
	float: right;
	margin-top: 12px;
	margin-right: 12px;
}
.code_box{
	width: 80%;
	height: 30px;
	line-height: 30px;
	font-size: 12px;
	margin: 30px auto;
}
.code_box > input{
	border: 1px solid #57c6d4;
	height: 30px;
	margin: 0 10px;
}
.gath_box{
	width: 80%;
	margin: 0 auto;
	line-height: 30px;
	height: 90px;
}
.gath_list{
	width: 80%;
	height: 100%;
	float: right;
	color: #333;
	overflow: auto;
}
.gath_list > li > span{
	width: 12px;
	height: 12px;
	border: 1px solid #57c6d4;
	position: relative;
	display: inline-block;
	border-radius: 50%;
	margin-right: 8px;
}
.gath_list > li.on{
	color: #57c6d4;
}

.gath_list > li.on > span:before{
	content: '';
	display: inline-block;
	width: 5px;
	height: 5px;
	background: #57c6d4;
	border-radius: 50%;
	position: absolute;
	top: 2.5px;
	left: 2.5px;
}
.btns{
	width: 50%;
	height: 30px;
	margin: 0px auto;
	margin-top: 50px;
}
.btns .q_btn{
	float: right;
}
.ask_mask{
	width: 100%;
	height: 100%;
	background: #fff;
	filter: alpha(opacity=60);
	opacity: .6;
	position: absolute;
	top: 0;
	left: 0;
	display: none;
}
.add_ask_for{
	width: 295px;
	height: 286px;
	background: #fff;
	position: absolute;
	bottom: 0;
	right: -295px;
	display: none;
}
.add_p{
	width: 90%;
	height: 30px;
	margin: 10px auto;
	line-height: 30px;
}
.add_p > label{
	width: 35%;
	text-align: right;
	display: inline-block;
	float: left;
}
.add_p > span{
	width: 65%;
	display: inline-block;
	float: right;
}
.add_p > span i{
	width: 50%;
	display: inline-block;
	text-align: right;
	position: relative;
	cursor: pointer;
}
.add_p > span > i:before{
	position: absolute;
	width: 13px;
	height: 13px;
	display: inline-block;
	content: '';
	border: 2px solid #57c6d4;
	border-radius: 50%;
	left: 23px;
	top: 8px;
}
.add_p > span > i.on:after{
	content: '';
	display: inline-block;
	position: absolute;
	width: 7px;
	height: 7px;
	background: #57c6d4;
	border-radius: 50%;
	left: 28px;
	top: 13px;
}
.add_p > input{
	width: 55%;
	height: 100%;
	border: 1px solid #57c6d4;
	float: right;
}
.add_ask_for .btns{
	width: 80%;
	margin-top: 20px;
}
.zf{
	display: none;
}
/* 提现申请 */

/* 账户流水 */
.ls_date{
	margin-top: 23px;
	display: none;
}
.ls_list{
	display: none;
}
.ls_list_info li{
	line-height: 40px;
	height: 40px;
	border-bottom: 1px solid #f7f7f7;
}
.ls_list_info span,
.ls_list > p > span{
	display: inline-block;
	text-align: center;
	width: 11.111%;
}
.date_span{
	width: 22.222% !important;
}
