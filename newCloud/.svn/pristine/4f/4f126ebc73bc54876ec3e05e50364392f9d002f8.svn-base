/* 收入明细 */
.mx_box{
	margin-top: 23px;
	height: 60px;
	font-size: 12px;
	display: none;
	line-height: 30px;
}
.mx_box input{
	width: 140px !important;
}
.mx_box > button{
	margin-left: 15px;
}
.mx_box > .fn_select{
