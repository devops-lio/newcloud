					msg(data.msg,false);
				}else if(data.code==203){
					msg(data.msg,false);
				}else if(data.code==204){
					msg(data.msg,false);
					$('.mask').css('display','none');
					$('.ask_for').css('display','none');
				}else if(data.code==205){
					msg(data.msg,false);
				}else if(data.code==206){
					msg(data.msg,false);
				}else if(data.code==207){
					msg(data.msg,false);
				}else if(data.code==208){
					msg(data.msg,false);
				}
			},
		});
}
