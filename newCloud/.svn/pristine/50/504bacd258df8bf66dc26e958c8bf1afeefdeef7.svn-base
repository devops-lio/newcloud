/**
* 处理银行卡号4为加一个空格
* 只能用作回显银行卡号
*/
function bankNumSpace(str){
	var arr=new Array(),
		str1='',
		len=parseInt(str.length/4);
	if(str.length!=16){
		for(var i=0;i<len+1;i++){
			if(i==len){
				arr.push(str.substr(i*4,4));
			}else{
				arr.push(str.substr(i*4,4)+" ");
			}
		}
	}else{
		for(var i=0;i<len;i++){
			if(i==len-1){
				arr.push(str.substr(i*4,4));
			}else{
				arr.push(str.substr(i*4,4)+" ");
			}
		}
	}
	for(var i=0;i<arr.length;i++){
		str1+=arr[i];
	}
	return str1;
}