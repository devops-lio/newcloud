/* 加载动画 */
.barcontainer {
    width: 100%;
    margin: 112px auto;
    margin-left: 170px;
    display: none;
}
.meter {
    position: absolute;
    width: 0px;
    height: 3px;
    border-right: 5px solid white;
    background-color: #5ce6e6;
    transition: width 0.5s;
    animation: loading 0.8s ease-out infinite;
}
@keyframes loading {
    0% {
        width: 0;
    }
    100% {
        opacity: 0;
        width: 100%;
    }
}
.loading{
	width: 100%;
	height: 100%;
	background: #fff;
	position: fixed;
	top: 0;
	left: 0px;
	z-index: 5;
	display:-webkit-box;
	-webkit-box-orient:horizontal; 
	-webkit-box-pack:center; 
	-webkit-box-align:center; 
	display:-moz-box; 
	-moz-box-orient:horizontal; 
	-moz-box-pack:center; 
	-moz-box-align:center; 
	display:-o-box; 
	-o-box-orient:horizontal; 
	-o-box-pack:center; 
	-o-box-align:center; 
	display:-ms-box; 
	-ms-box-orient:horizontal; 
	-ms-box-pack:center; 
	-ms-box-align:center; 
	display:box; 
	box-orient:horizontal; 
	box-pack:center; 
	box-align:center;
	display: none;
}
#text{
	width: 200px;
	height: 50px;
	font-size: 24px;
	font-weight: bold;
	text-align: center;
	color: #ccc;
}
#text>span{
	width: 50px;
	height: 50px;
	text-align: center;
	line-height: 50px;
	margin: 0 2px;
}
#text>p{
	font-size: 14px;
	font-weight: normal;
	color: #999;
}
#text>span.nth{
	animation: scale 0.9s ease-in-out;
}
@keyframes scale{
	0%{transform:scale(1);}
	50%{transform:scale(1.5);}
	100%{transform:scale(1);}
}