	background: none;
	margin-top: 12px;
	cursor: pointer;
}
.LorR>i{
	color: #6bbdec;
}
.LorR>i:nth-child(1){
	float: left;
}
.LorR>i:nth-child(2){
	float: right;
}
.indicate{
	height: 5px;
	width: 100%;
	background: #f7f7f7;
	display: block;
	clear: both;
	position: relative;
}
.indicate>span{
	width: 108px;
	height: 100%;
	background: #5abef7;
	display: inline-block;
	position: absolute;
}
.fessContent{
	margin-top: 50px;
	color: #666;
}
.charge>span,.fuse>span{
	float: left;
	line-height: 25px;
}
.yesOrNos,
.yesOrNo{
	width: 57px;
	height: 23px;
	background: #6bbdec;
	border-radius: 25px;
	display: inline-block;
	margin-left: 20px;
	line-height: 25px;
	position: relative;
	color: #f2f2f2;
	padding: 1px;
	cursor: pointer;
	font-size: 12px;
	float: left;
}
.yesOrNos>.yes,
.yesOrNo>.yes{
	margin-left: 7px;
	float: left;
}
.yesOrNos>.no,
.yesOrNo>.no{
	margin-right: 7px;
	float: right;
	display: none;
	color: #999;
}
.yesOrNos>.bal,
.yesOrNo>.bal{
	width: 23px;
	height: 23px;
	position: absolute;
	background: #f2f2f2;
	border-radius: 50%;
	top: 1px;
	right: 1px;
	box-shadow: 0 0 5px #666;
	display: inline-block;
}
.yesOrNos.on,
.yesOrNo.on{
	background: #ddd;
}
.yesOrNos.on>.yes,
.yesOrNo.on>.yes{
	display: none;
}
.yesOrNos.on>.no,
.yesOrNo.on>.no{
	display: inline-block;
}
.yesOrNos.on>.bal,
.yesOrNo.on>.bal{
	right: 35px;
}

