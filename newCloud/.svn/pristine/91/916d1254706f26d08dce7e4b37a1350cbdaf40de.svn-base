*{
	margin: 0;
	padding: 0;
}
html,body{
	font-family: "Microsoft Yahei","icon" !important;
	font-size: 14px;
	height: 100%;
	position: relative;
	overflow-y: auto;
	overflow-x: hidden;
}
ul,li{
	list-style: none;
}
button{
	font-family: Microsoft Yahei;
}
input{
	cursor: text !important;
}
::-webkit-input-placeholder { 
	color: #b3b3b3 !important; 
} 
.ui-header{
	position: fixed;
	width: 100%;
	height: 60px;
	background: #57c6d4;
	z-index: 10;
}
.ui-header>h1{
	line-height: 60px;
	position: relative;
	color: #fff;
	margin-left: 26px;
	font-size: 20px;
}
.ui-header>h1>i,.ui-header>h1>span{
	position: absolute;
}
.icon-logo{
	left: 0;
	top: 10px;
	font-size: 50px;
}
.icon-solar{
	left: 60px;
	top: -85px;
	font-size: 140px;
	color: #e7e7e7;
}
.sun{
	left: 210px;
}
.back{
	left: 270px;
	font-weight: normal;
}
.admin{
	float: right;
	color: #fff;
	font-size: 20px;
	height: 100%;
	cursor: pointer;
	padding: 0 10px;
	margin-right: 12px;	
}
.icon-ask{
	float: right;
	margin-right: 60px;
	width: 40px;
	height: 60px;
	line-height: 60px;
	color: #fff;
	display: inline-block;
	text-align: center;
	cursor: pointer;
}
.icon-ask:hover{
	background: #4cadb9;
}
.admin>i.icon-admin{
	float: left;
	margin-right: 8px;
	margin-top: 20px;
}
.admin>i.icon-down{
	float: right;
	margin-top: 20px;
	font-size: 14px;
	margin-left: 8px;
}
.menu{
	width: 165px;
	background: #57c6d4;
	position: absolute;
	top: 60px;
	right: 112px;
	display: none;
}
.menu > li{
	width: 100%;
	height: 30px;
	text-align: center;
	line-height: 30px;
	color: #fff;
	cursor: pointer;
}
.menu > li.exit{
	background: url(../img/exit.png) no-repeat 12px center;
	background-size: 15px;
}
.menu > li.exit:hover{
	background: #4cadb9 url(../img/exit.png) no-repeat 12px center;
	background-size: 15px;
}
.adname{
	display: inline-block;
	width: 100px;
	font-size: 14px;
	line-height: 60px;
	overflow:ellipsis;
	text-overflow: ellipsis;
	white-space:nowrap;
	overflow:hidden;
	height: 60px;
}
.admin:hover{
	background: #4cadb9;
}
.ui-nav{
	position: fixed;
	width: 170px;
	height: 100%;
	background: #e8e8e8;
	z-index: 9;
}
.ui-nav>.list{
	width: 100%;
	height: 54px;
	line-height: 54px;
	font-size: 14px;
	font-weight: normal;
	color: #666;
	cursor: pointer;
}
.ui-nav>.list>a{
	width: 100%;
	height: 100%;
	 display: inline-block; 
	text-decoration: none;
	color: #9f9da9;
}
 
.ui-nav>.list>a>i{
	color: #9f9da9;
	font-size: 18px;
	margin: 0 16px 0 20px;
	width: 20px;
	display: inline-block;
}
.ui-nav>.list>a>i.icon-goLeft{
	font-size: 12px;
}
.ui-nav>.list>a>i.icon-goDown{
	font-size: 12px;
}
.list:nth-child(1){
	margin-top: 60px;
}
.list:hover{
	background: #ddd;
}
.list.on{
	background: #c2c2c2;
	color: #fff;
}
.list.on>a{
	color: #fff;
}
.ui-nav>ul{
	font-size: 12px;
}
.ui-nav>ul>li{
	height: 30px;
	line-height: 30px;
	width: 170px;
	text-align: center;
	color: #666;
	cursor: pointer;
}
.ui-nav>ul>li>a{
	color: #666;
	text-decoration: none;
}
.ui-nav>ul>li.on{
	color: #333;
	font-weight: bold;
}
.ui-nav>ul>li:hover{
	color: #333;
}
.container{
	position: absolute;
	width: 100%;
	height: 100%;
}
.content{
	margin-left: 170px;
	margin-top: 60px;
}
.operation{
	display: block;
}
/* --------------------------------------------- */
.siteListTitle{
	margin-top: 15px;
	margin-left: 2%;
	color: #666;
	display: inline-block;
}
.plistT{
	width: 95%;
	margin-left: 2%;
	margin-top: 15px;
}
.plistT>span{
	float: left;
	line-height: 38px;
	background: #f7f7f7;
	width: 10.9%;
	text-align: center;
	color: #666;
	font-weight: bold;
}
.plistContant{
	margin-left: 2%;
	width: 95%;
	font-size: 12px;
}
.plistContant>li{
	float: left;
	width: 10.9%;
	text-align: center;
	line-height: 40px;
	height: 39px;
	border-bottom: 1px solid #f7f7f7;
	position: relative;
}
.plistContant>li>p{
	width: 80%;
	height: 100%;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
	float: left;
}
.editFloa,
.floa{
	width: 188px;
	height: 120px;
	position: absolute;
	box-shadow: 0 0 5px rgba(14,33,35,0.53);
	background: #fff;
	z-index: 8;
	top: 27px;
	left: 88px;
	display: none;
}
#addfloa{
	width: 248px;
	height: 180px;
	position: absolute;
	box-shadow: 0 0 5px rgba(14,33,35,0.53);
	background: #fff;
	z-index: 8;
	top: 27px;
	left: 88px;
	display: none;
}
.addfloa>input{
	width: 200px;
	height: 34px;
	margin-top: 18px;
	border: 1px solid #ccc;
}
.floa>input,.editFloa>input{
	width: 140px;
	height: 34px;
	margin-top: 18px;
	border: 1px solid #ccc;
}
.floa>span,
.editFloa>span{
	color: red;
	height: 15px;
	font-size: 12px;
	display: block;
	margin-top: -10px;
}
.addfloa>span{
	color: red;
	height: 15px;
	font-size: 12px;
	display: block;
	margin-top: -10px;
	margin-left: -5px;
}
.btno,
.btnnum,
.btn{
	width: 60px;
	height: 30px;
	background: #6bbdec;
	border: none;
	margin-top: 18px;
	color: #fff;
	cursor: pointer;
}
#addfloa>p>button{
	margin-top: 18px !important;
}
.btn:nth-child(2){
	background: #fff;
	color: #6bbdec;
}
.plistContant>li>span{
	height: 30px;
	border-bottom: 1px dashed #6bbdec;
	color: #6bbdec;
	cursor: pointer;
}
.plistContant:hover>li{
	background: #f1f1f1;
}
.deviceBtn{
	background: #6bbdec;
	border: none;
	width: 72px;
	height: 26px;
	border-radius: 26px;
	color: #fff;
	cursor: pointer;
	font-size: 12px;
	line-height: 26px;
}
.plistContant>li>i{
	color: #fff;
	margin-left: 0px;
	cursor: pointer;
}
.plistContant:hover>li>i{
	color: #6bbdec;
}
.plistContant>li>.yesOrNo{
	top: 7px !important;
	left: 12% !important;
}
/* --------------------------------------------- */

.operation>.title,
.billing>.title,
.place>.title,
.user-income>.title,
.user-pay>.title{
	height: 54px;
	color: #57c6d4;
	font-size: 14px;
	line-height: 54px;
	width: 100%;
	background: #f7f7f7;
	padding-left: 2%;
}
  .operation > .site,
    .billing > .site,
.user-income > .site,
   .user-pay > .site{
	margin-top: 27px;
	margin-left: 2%;
}
  .operation > .site > li,
    .billing > .site > li,
.user-income > .site > li{
	float: left;
	margin-right: 80px;
	line-height: 34px;
	position: relative;
	color: #b2b2b2;
	margin-left: 5px;
}
.user-pay > .site > li{
	float: left;
	line-height: 34px;
	position: relative;
	color: #b2b2b2;
	margin-left: 5px;
}
  .operation > .site > li > span,
    .billing > .site > li > span,
    .billing > .site > li > input,
.user-income > .site > li > span,
   .user-pay > .site > li > span{
	margin-left: 12px;
}
.user-college{
	margin-left: 12px;
}
.site>li>span.college,
.site>li>span.college,
.site>li>input.college,
.user-college{
	width: 158px;
	height: 32px;
	border: 1px solid #57c6d4;
	display: inline-block;
	text-align: center;
	line-height: 34px;
	cursor: pointer;
	color: #666;
}
.ui-addUser{
	width: 120px;
	height: 34px;
	line-height: 34px;
	background: #57c6d4;
	border: none;
