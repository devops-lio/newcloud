	display: block;
	margin-left: 2%;
	margin-top: 80px;
}
.ui-addUser>i{
	margin: 0 15px;
}
.site>li>input.college{
	cursor: text;
}
.site>li>.pullDs,
.site>li>.pullD{
	display: none;
	position: absolute;
	top: 34px;
	color: #666;
	right: 0;
	background: #ebf1f2;
	font-size: 12px;
	z-index: 50;
}
.site>li>.pullDs>li,
.site>li>.pullD>li{
	width: 160px;
