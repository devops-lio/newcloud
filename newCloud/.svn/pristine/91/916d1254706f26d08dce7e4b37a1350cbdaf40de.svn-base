	line-height: 100px;
}
.both>div{
	position: absolute;
	bottom: 0;
	left: 27%;
	width: 46%;
	display: inline-block;
	height: 30px;
	line-height: 30px;
	border-bottom: 1px dashed #4bc1f8;
	color: #4bc1f8;
	text-align: center;
}
.float{
	width: 200px;
	height: 63px;
	background: #2d3031;
	color: #e8e8e8;
	font-size: 12px;
	text-align: center;
	line-height: 30px;
	border-radius: 4px;
	position: absolute;
	top: 30px;
	left: 70%;
	display: none;
}
.both>div:hover>p{
	display: block;
}
#perception{
	position: absolute;
	padding: 0;
	display: block;
	left: 0;
	top: 0;
	z-index: 5;
}
#tryOut{
	position: absolute;
	padding: 0;
	display: block;
	left: 0;
	top: 0;
	z-index: 5;
}
#register{
	position: absolute;
	padding: 0;
	display: block;
	left: 0;
	top: 0;
}
.occupie{
	margin-left: 2%;
	margin-top: 5px;
	width: 95%;
	position: relative;
}
#pie{
	position:absolute;
	left: 500px;
	top: 144px;
}
.occupie>.userOcc{
	color: #4d4d4d;
	font-weight: bold;
}
#pieChart{
	margin-left: 100px;
}
.income{
	color: #4d4d4d;
	border-bottom: 5px solid #f7f7f7;
	line-height: 30px;
	margin-left: 2%;
	width: 95%;
	font-weight: bold;
}
.income>p{
	float: right;
	margin-right: 20px;
	cursor: pointer;
	font-weight: normal;
	color: #999;
}
.income>p>span{
	width: 8px;
	height: 8px;
	display: inline-block;
	border: 1px solid #b2b2b2;
	margin-right: 5px;
	position: relative;
}
.income>p>span>i{
	position: absolute;
	top: -1px;
	left: -2px;
	color: #fff;
	font-size: 10px;
	display: none;
}
.income>p>span.on{
	background: #57c6d4;
	border: none;
	width: 10px;
	height: 10px;
}
.income>p>span.on>i{
	display: block;
}
.emphasis,.numPeople{
	margin-left: 2%;
	width: 46%;
	height: 340px;
	float: left;
	margin-top: 20px;
	font-size: 14px;
	position: relative;
}

.emphasis>span,.numPeople>span{
	color: #4d4d4d;
	font-weight: bold;
}
.emphasis>h4,.numPeople>h4{
	width: 100%;
	line-height: 40px;
	background: #f7f7f7;
	font-weight: normal;
	font-size: 14px;
	color: #666;
	margin-top: 10px;
}
.emphasis>h4>span,.numPeople>h4>span,.emphasis>div>span,.numPeople>div>span{
	width: 33.33%;
	height: 100%;
	display: inline-block;
	text-align: center;
}
.emphasis>div>span>a,.numPeople>div>span>a{
	width: 55%;
	line-height: 34px;
	display: inline-block;
	background: #57c6d4;
	color: #fff;
	text-decoration: none;
}
.emphasis>div,.numPeople>div{
	width: 100%;
	line-height: 40px;
	font-size: 12px;
	color: #57c6d4;
	position: relative;
}
.emphasis>div.sale:hover{
	background: #ddd;
}
.emphasis .detail{
	width: 60%;
	background: #e8e8e8;
	position: absolute;
	left: 0;
	top: 0;
	z-index: 10;
	display: none;
}
.detail>p{
	border-bottom: 1px solid #fff;
}
.detail>p:nth-child(1){
	background: #343a3d;
}
.detail>p>span{
	width: 50%;
	text-align: center;
	display: inline-block;
	color: #666;
	font-size: 12px;
}
.detail>p:nth-child(1)>span{
	color: #e8e8e8;
}
.emphasis>div:hover>.detail{
	display: block;
}
.numPeople>.allNum{
	width: 180px;
	display: inline;
	color: #999;
	font-size: 14px;
	font-weight: normal;
	position: absolute;
	top: -4px;
	right: 0;
}
.numPeople>.allNum>span{
	color: #cabc43;
	font-size: 20px;
	width: 40px;
	margin-left: 10px;
}
.siteList{
	margin-left: 2%;
	width: 95%;
	margin-top: 80px;
}
.siteList>p{
	width: 100%;
	color: #b2b2b2;
}
.siteListAdd{
	width: 293px;
	margin-top: 10px;
	float: left;
}
.siteListAdd>li{
	width: 100%;
	cursor: pointer;
	border-bottom: 1px solid #f7f7f7;
	font-size: 12px;
	position: relative;
}
.siteListAdd>li:hover{
	background: #f5f5f5;
}
.siteListAdd>li>i{
	float: right;
	position: absolute;
	right: 0;
	top: 12px;
	color: #fff;
	font-size: 18px;
	display: none;
}
.siteListAdd>li.on{
	background: #ebebeb;
}
.siteListAdd>li.on>i{
	display: block;
}
.siteListAdd>li:nth-child(1){
	background: #f7f7f7;
	cursor: default;
	color: #666;
	font-weight: bold;
	font-size: 14px;
}
.siteListAdd>li:last-child{
	border-bottom: none;
}
.siteListAdd>li>span{
	height: 38px;
	line-height: 38px;
	width: 50%;
	text-align: center;
	display: inline-block;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}

.paging{
	margin-top: 25px;
	float: right;
	margin-right: 30px;
}

.deviceList>.paging{
	margin-top: 25px;
	float: right;
	
}

.paging>li,
.deviceList>.paging>li{
	float: left;
	margin: 0 3px;
	width: 18px;
	height: 18px;
	text-align: center;
	line-height: 18px;
	color: #fff;
	cursor: pointer;
	background: #6bbdec;
}
.paging>li.on,
.deviceList>.paging>li.on{
	font-weight: bold;
	color: #666;
	background: none;
	cursor: default;
}
.paging>li.zong,
.deviceList>.paging>li.zong{
	cursor: default;
}
.paging>li:nth-child(1),
.deviceList>.paging>li:nth-child(1){
	width: 60px;
	color: #808080;
}
.paging>li:nth-child(1),
.paging>li:nth-child(2),
.paging>li:last-child,
.deviceList>.paging>li:nth-child(1),
.deviceList>.paging>li:nth-child(2),
.deviceList>.paging>li:last-child{
	background: none;
}
.paging>li>i{
	color: #6bbdec;
	cursor: pointer;
}
.fessNorm{
	margin-left: 170px;
	float: left;
}
