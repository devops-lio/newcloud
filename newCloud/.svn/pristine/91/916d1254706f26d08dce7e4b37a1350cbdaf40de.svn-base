}
.site>li>span.money{
	color: #cabc43;
	font-size: 20px;
}
.site>li>button{
	width: 80px;
	height: 36px;
	background: #47bfca;
	border: none;
	color: #fff;
	margin-left: -60px;
	cursor: pointer;
}
.ui-userAccount{
	margin-left: 2%;
	margin-top: 20px;
	font-size: 14px;
}
.ui-userAccount>p{
	height: 32px;
	line-height: 32px;
}
.ui-userAccount>p>label{
	color: #999;
	margin-right: 10px;
	letter-spacing: 3px;
}
.ui-userAccount>p>span{
	margin-left: 10px;
	color: #57c6d4;
	letter-spacing: 3px;
}
.recharge{
	width: 120px;
	height: 34px;
	color: #fff;
	border: none;
	background: #57c6d4;
	margin-top: 20px;
	cursor: pointer;
}
.premises>ul>.group>span{
	margin-left: 30px !important;
}
.premises>ul>li>p{
	display: inline;
	margin-left: 30px;
}
.userMode{
	clear: both;
	margin-left: 2%;
	margin-top: 118px;
	color: #4d4d4d;
	font-weight: bold;
}
.contrast{
	width: 95%;
	margin-left: 2%;
	border-bottom: 1px solid #f7f7f7;
	margin-top: 14px;
}
.contrast>p{
	line-height: 38px;
	height: 38px;
	width: 100%;
}
.contrast>.conti{
	background: #f7f7f7;
}
.contrast>p>span{
	width: 220px;
	text-align: center;
	display: inline-block;
	margin: 0 35px;
}
.contrast>.conti>span{
	color: #666;
}
.contrast>.price>span{
	color: #57c6d4;
}
.userAnn{
	margin-left: 2%;
	margin-top: 45px;
	color: #4d4d4d;
	font-weight: bold;
}
.annular{
	margin-left: 2%;
	margin-top: 5px;
	height: 300px;
	width: 95%;
}
.both{
	width: 200px;
	height: 220px;
	position: relative;
	cursor: pointer;
	float: left;
	margin: 0 80px;
}
.round{
	width: 100px;
	height: 100px;
	border: 20px solid #f2f2f2;
	border-radius: 50%;
	position: absolute;
	top: 30px;
	left: 30px;
	color: #4c4c4c;
	text-align: center;
