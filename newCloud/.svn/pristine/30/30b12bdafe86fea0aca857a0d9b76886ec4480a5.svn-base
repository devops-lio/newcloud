/* 开关样式 */
.swicth{
	width: 52px;
	height: 20px;
	line-height: 20px;
	color: #fff;
	position: relative;
	display: inline-block;
	border-radius: 20px;
	font-size: 12px;
	cursor: pointer;
}
.swicth.m{
	background: #ccc;
}
.swicth.d{
	background: #57c6d4;
}
.swicth.d:before{
	content: 'ON';
	position: absolute;
	left: 4px;
}
.swicth.m:after{
	content: 'OFF';
	position: absolute;
	right: 4px;
}
.swicth > i{
	width: 20px;
	height: 20px;
	display: inline-block;
	border-radius: 50%;
	background: #fff;
	position: absolute;
	right: 1px;
}
/* 开关样式 */